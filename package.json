{
  "name": "sports-card",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"frontend,backend\" -c \"blue,green\" \"npm run dev --prefix frontend\" \"cd Backend && uvicorn main:app --reload --port 8000\"",
    "frontend": "npm run dev --prefix frontend",
    "backend": "cd Backend && uvicorn main:app --reload --port 8000",
    "install:all": "npm install && npm install --prefix frontend"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "playwright": "^1.58.2"
  }
}
